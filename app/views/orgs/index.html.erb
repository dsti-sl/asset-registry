<p id="notice"><%= notice %></p>

<h1>Orgs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Lat</th>
      <th>Lon</th>
      <th>Kind</th>
      <th>Region</th>
      <th>District</th>
      <th>Chiefdom</th>
      <th>Section</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @orgs.each do |org| %>
      <tr>
        <td><%= org.name %></td>
        <td><%= org.address %></td>
        <td><%= org.lat %></td>
        <td><%= org.lon %></td>
        <td><%= org.kind %></td>
        <td><%= org.region %></td>
        <td><%= org.district %></td>
        <td><%= org.chiefdom %></td>
        <td><%= org.section %></td>
        <td><%= link_to 'Show', org %></td>
        <td><%= link_to 'Edit', edit_org_path(org) %></td>
        <td><%= link_to 'Destroy', org, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Org', new_org_path %>
